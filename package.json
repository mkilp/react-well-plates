{
  "name": "react-well-plates",
  "version": "2.0.5",
  "description": "Render well plates in react",
  "main": "lib/src/index.js",
  "module": "lib-es/src/index.js",
  "types": "lib-es/src/index.d.ts",
  "files": [
    "lib/src",
    "lib-es/src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc && tsc -p tsconfig.es.json",
    "clean": "rimraf lib lib-es",
    "npm:publish": "npm t && npm publish --access public && git push --follow-tags",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run deploy",
    "watch": "tsc -p tsconfig.es.json -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "npm run build-storybook && gh-pages -d storybook-static",
    "eslint": "eslint --ext ts,tsx src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "npm run eslint && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/react-well-plates.git"
  },
  "keywords": [],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/react-well-plates/issues"
  },
  "homepage": "https://github.com/zakodium/react-well-plates#readme",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always",
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-cheminfo-react": "^4.0.0",
    "eslint-config-cheminfo-typescript": "^5.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "well-plates": "^4.1.0"
  }
}
