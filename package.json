{
  "name": "react-well-plates",
  "version": "5.0.0",
  "description": "Render well plates in react",
  "main": "lib/src/index.js",
  "module": "lib-es/src/index.js",
  "types": "lib-es/src/index.d.ts",
  "files": [
    "lib/src",
    "lib-es/src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc && tsc -p tsconfig.es.json",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-es",
    "prepack": "npm run build",
    "dev": "start-storybook --no-dll -p 6006",
    "build-storybook": "build-storybook --no-dll",
    "eslint": "eslint --ext ts,tsx src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "test": "npm run eslint && npm run check-types && npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium-oss/react-well-plates.git"
  },
  "keywords": [],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium-oss/react-well-plates/issues"
  },
  "homepage": "https://github.com/zakodium-oss/react-well-plates#readme",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-storysource": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/react": "^17.0.43",
    "@zakodium/eslint-config": "^5.1.1",
    "eslint": "^8.12.0",
    "prettier": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "well-plates": "^6.0.3"
  },
  "volta": {
    "node": "16.14.2"
  }
}
