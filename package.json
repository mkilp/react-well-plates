{
  "name": "react-well-plates",
  "version": "0.2.3",
  "description": "Render well plates in react",
  "main": "lib/src/index.js",
  "module": "lib-es6/src/index.js",
  "types": "lib-es6/src/index.d.ts",
  "files": [
    "lib/src",
    "lib-es/src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc && tsc -p tsconfig.es.json",
    "clean": "rimraf lib lib-es",
    "npm:publish": "npm publish --access public && git push --follow-tags",
    "prepublishOnly": "npm run build",
    "watch": "tsc -p tsconfig.es.json -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "npm run build-storybook && gh-pages -d storybook-static",
    "eslint": "eslint --ext ts,tsx src",
    "eslint-fix": "npm run eslint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/react-well-plates.git"
  },
  "keywords": [],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/react-well-plates/issues"
  },
  "homepage": "https://github.com/zakodium/react-well-plates#readme",
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-info": "^5.0.1",
    "@storybook/addon-knobs": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addons": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "@types/react": "^16.8.7",
    "@typescript-eslint/parser": "^1.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.1",
    "eslint-plugin-react-hooks": "^1.5.0",
    "gh-pages": "^2.0.1",
    "react": "^16.8.1",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": "^16.8.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "well-plates": "^3.2.2"
  }
}
