{
  "name": "react-well-plates",
  "version": "0.0.0",
  "description": "Render well plates in react",
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "files": [
    "lib",
    "lib-es"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc && tsc -p tsconfig.es.json",
    "clean": "rimraf lib lib-es",
    "npm:publish": "npm publish --access public && git push --follow-tags",
    "prepublishOnly": "npm run build",
    "watch": "tsc -p tsconfig.es.json -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/react-well-plates.git"
  },
  "keywords": [],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/react-well-plates/issues"
  },
  "homepage": "https://github.com/zakodium/react-well-plates#readme",
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "well-plates": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-info": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/react": "^16.8.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "react": "^16.8.1",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": "^16.8.1",
    "typescript": "^3.3.3",
    "well-plates": "^3.0.0"
  }
